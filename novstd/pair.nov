// Types

struct Pair{T1, T2} = T1 first, T2 second

// Conversions

fun string{T1, T2}(Pair{T1, T2} p)
  "(" + p.first.string() + "," + p.second.string() + ")"

// Tests

assert(
  p = Pair(42, "hello world");
  p == Pair(42, "hello world") &&
  p != Pair(42, "hello") &&
  p != Pair(1337, "hello world"))

assert(Pair(42, "hello world").string() == "(42,hello world)")
