{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "eval",
      "preLaunchTask": "build debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bin/eval",
      "args": [
        "conWrite(string(1 + 1))"
      ],
      "cwd": "${workspaceFolder}",
      "terminal": "integrated",
      "stopOnEntry": false
    },
    {
      "name": "lexdiag",
      "preLaunchTask": "build debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bin/lexdiag",
      "args": [
        "analyze",
        "conWrite(string(x + 42 * y))"
      ],
      "cwd": "${workspaceFolder}",
      "terminal": "integrated",
      "stopOnEntry": false
    },
    {
      "name": "parsediag",
      "preLaunchTask": "build debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bin/parsediag",
      "args": [
        "analyze",
        "conWrite(string(x + 42 * y))"
      ],
      "cwd": "${workspaceFolder}",
      "terminal": "integrated",
      "stopOnEntry": false
    },
    {
      "name": "progdiag",
      "preLaunchTask": "build debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bin/progdiag",
      "args": [
        "analyze",
        "fun add(int a, int b) -> int a + b conWrite(string(add(1, 2) == 3))"
      ],
      "cwd": "${workspaceFolder}",
      "terminal": "integrated",
      "stopOnEntry": false
    },
    {
      "name": "asmdiag",
      "preLaunchTask": "build debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bin/asmdiag",
      "args": [
        "analyze",
        "fun add(int a, int b) -> int a + b conWrite(string(add(1, 2) == 3))"
      ],
      "cwd": "${workspaceFolder}",
      "terminal": "integrated",
      "stopOnEntry": false
    },
    {
      "name": "tests",
      "preLaunchTask": "build debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/bin/tests",
      "cwd": "${workspaceFolder}",
      "terminal": "integrated",
      "stopOnEntry": false
    }
  ]
}
